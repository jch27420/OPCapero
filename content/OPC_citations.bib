@article{liang_identification_2007,
	title = {Identification of a {Pool} of {Non}-{Pumping} {Na}/{K}-{ATPase}},
	volume = {282},
	issn = {0021-9258},
	doi = {10.1074/jbc.M609181200},
	abstract = {Recent studies have ascribed many non-pumping functions to the Na/K-ATPase. Here, we present experimental evidence demonstrating that over half of the plasma membrane Na/K-ATPase in LLC-PK1 cells is performing cellular functions other than ion pumping. This "non-pumping" pool of Na/K-ATPase, like the pumping pump, binds ouabain. Depletion of either cholesterol or caveolin-1 moves some of the "non-pumping" Na/K-ATPase into the pumping pool. Graded knock-down of the alpha1 subunit of the Na/K-ATPase eventually results in loss of this "non-pumping" pool while preserving the pumping pool. Our prior studies indicate that a loss of the non-pumping pool is associated with a loss of receptor function as evidenced by the failure of ouabain administration to induce the activation of Src and/or ERK. Therefore, our new findings suggest that a substantial amount of surface-expressed Na/K-ATPase, at least in some types of cells, may function as non-canonical ouabain-binding receptors.},
	number = {14},
	journal = {The Journal of Biological Chemistry},
	author = {Liang, Man and Tian, Jiang and Liu, Lijun and Pierre, Sandrine and Liu, Jiang and Shapiro, Joseph and Xie, Zi-Jian},
	month = apr,
	year = {2007},
	note = {\_eprint: 17296611
\_eprinttype: pmid},
	keywords = {Animals, Humans, Sodium-Potassium-Exchanging ATPase, Ouabain, Cell Membrane, Enzyme Activation, Cell Line, src-Family Kinases, Extracellular Signal-Regulated MAP Kinases, Caveolin 1, Protein Subunits},
	pages = {10585--10593},
	annote = {\_eprint: 17296611 \_eprinttype: pmid},
}

@article{von_lintig_filling_2000,
