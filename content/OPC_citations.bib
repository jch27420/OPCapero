	note = {Place: United States},
	keywords = {Animals, Mutation, Mice, Phenotype, Vision, Ocular/*physiology, Mice, Knockout, *Proteins, Carrier Proteins, cis-trans-Isomerases, Esters, Eye Proteins/genetics/*physiology, Microscopy, Electron, Retina/*physiology/ultrastructure, Retinal Rod Photoreceptor Cells/metabolism/physiology, Rhodopsin/metabolism, Vitamin A/*biosynthesis/metabolism},
	pages = {344--351},
}

@article{palczewski_chemistry_2012,
	title = {Chemistry and biology of vision.},
	volume = {287},
	issn = {1083-351X 0021-9258 0021-9258},
	doi = {10.1074/jbc.R111.301150},
	abstract = {Visual perception in humans occurs through absorption of electromagnetic radiation  from 400 to 780 nm by photoreceptors in the retina. A photon of visible light  carries a sufficient amount of energy to cause, when absorbed, a cis,trans-geometric  isomerization of the 11-cis-retinal chromophore, a vitamin A derivative bound to  rhodopsin and cone opsins of retinal photoreceptors. The unique biochemistry of  these complexes allows us to reliably and reproducibly collect continuous visual  information about our environment. Moreover, other nonconventional retinal opsins  such as the circadian rhythm regulator melanopsin also initiate light-activated  signaling based on similar photochemistry.},
	language = {eng},
	number = {3},
	journal = {The Journal of biological chemistry},
	author = {Palczewski, Krzysztof},
	month = jan,
	year = {2012},
	pmid = {22074921},
	pmcid = {PMC3265841},
	keywords = {Animals, Humans, Mice, Vision, Ocular/*physiology, Retinaldehyde/*metabolism, *Photons, Circadian Rhythm/physiology, Cone Opsins/*metabolism, Photoreceptor Cells, Vertebrate/*metabolism, Rhodopsin/*metabolism, Rod Opsins/metabolism, Visual Perception/physiology},
	pages = {1612--1619},
}

@article{tanabe_primary_1985,
	title = {Primary structure of the alpha-subunit of transducin and its relationship to ras  proteins.},
	volume = {315},
	issn = {0028-0836 0028-0836},
	doi = {10.1038/315242a0},
	abstract = {A group of membrane-associated guanine nucleotide binding proteins (G-proteins) are  essential for transducing signals generated at cell-surface receptors into changes  in cellular function and metabolism. These proteins are a complex of three subunits  designated alpha, beta and gamma. The alpha-subunit is responsible for binding  guanine nucleotides and seems to be characteristic of each protein. Transducin, a  member of this protein family, mediates visual transduction by coupling the signal  of photolysed rhodopsin with activation of a cyclic GMP phosphodiesterase. We have  now cloned and sequenced the complementary DNA encoding the alpha-subunit of bovine  retinal transducin and from this we have deduced the complete amino-acid sequence.  The transducin alpha-subunit shares several homologous amino-acid sequences with ras  gene products. The homologous segments correspond mostly to the regions thought to  be involved in the guanine nucleotide binding and GTPase activity of ras proteins  and to the ADP-ribosylation sites of the transducin alpha-subunit.},
	language = {eng},
	number = {6016},
	journal = {Nature},
	author = {Tanabe, T. and Nukada, T. and Nishikawa, Y. and Sugimoto, K. and Suzuki, H. and Takahashi, H. and Noda, M. and Haga, T. and Ichiyama, A. and Kangawa, K.},
	month = may,
