}

@article{daglia_polyphenols_2014,
	title = {Polyphenols: {Well} beyond the {Antioxidant} {Capacity}: {Gallic} {Acid} and {Related} {Compounds} as {Neuroprotective} {Agents}: {You} {Are} {What} {You} {Eat}!},
	volume = {15},
