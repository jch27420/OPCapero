	note = {Number: 7008
Publisher: Nature Publishing Group},
	pages = {549--552},
	file = {Snapshot:C\:\\Users\\Jeffrey Howard\\Zotero\\storage\\DX4DX7CB\\nature02888.html:text/html},
}

