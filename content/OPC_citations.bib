	author = {Palczewski, K. and Buczyłko, J. and Kaplan, M. W. and Polans, A. S. and Crabb, J. W.},
	month = jul,
	year = {1991},
	pmid = {2071581},
	note = {Place: United States},
	keywords = {Animals, Amino Acid Sequence, Molecular Sequence Data, Protein Conformation, Kinetics, Enzyme Activation, Cattle, Cell Membrane/enzymology, Rod Cell Outer Segment/*enzymology, *Eye Proteins, Cytoplasm/enzymology, G-Protein-Coupled Receptor Kinase 1, Protein Kinases/chemistry/isolation \& purification/*metabolism, Transducin/isolation \& purification/metabolism},
	pages = {12949--12955},
}

@article{cooper_energy_1979,
	title = {Energy uptake in the first step of visual excitation.},
	volume = {282},
	issn = {0028-0836 0028-0836},
	doi = {10.1038/282531a0},
	abstract = {Perception of light by the retina starts with the absorption of a photon by 11-cis  retinal, which is covalently incorporated into the membrane-bound protein,  rhodopsin. The initial result of photon capture is the very rapid formation of a  red-shifted species, bathorhodopsin (also known as prelumirhodopsin), which is  (meta-)stable at liquid nitrogen temperature but which decomposes at higher  temperatures, in the dark, through a series of intermediate stages, resulting in the  release of all-trans retinal from the apoprotein, opsin. Bathorhodopsin formation is  the only photochemical step in the overall reaction and, therefore, merits  investigation. Several models for the process have been proposed, and have been  critically reviewed, although no consensus yet exists as to the nature or mechanism  of formation of the batho intermediate. I report here on the first direct  measurement of photon energy uptake during bathorhodopsin formation from bovine  rhodopsin, and on its possible significance.},
	language = {eng},
	number = {5738},
	journal = {Nature},
	author = {Cooper, A.},
	month = nov,
	year = {1979},
	pmid = {503236},
	note = {Place: England},
	keywords = {Animals, Energy Metabolism, Protein Conformation, Cattle, Temperature, Rhodopsin/*metabolism, *Vision, Ocular, Retinal Pigments/*metabolism, Photochemistry, Photoreceptor Cells/*physiology, Thermodynamics},
	pages = {531--533},
}

@article{kanai_retinol-binding_1968,
	title = {Retinol-binding protein: the transport protein for vitamin {A} in human plasma.},
	volume = {47},
	issn = {0021-9738 0021-9738},
	doi = {10.1172/JCI105889},
	abstract = {Vitamin A circulates in human plasma as retinol bound to a specific transport  protein. This protein differs from the known low and high density plasma  lipoproteins and has a hydrated density greater than 1.21. In order to study this  protein, volunteers were injected intravenously with retinol-15-(14)C. Plasma was  collected 1-3 days later, and the purification of retinol-binding protein (RBP) was  monitored by assaying for (14)C and also by following the fluorescence of the  protein-bound retinol. Purification of RBP was effected by the sequence: Cohn  fractionation, chromatography on columns of Sephadex G-200 and diethylaminoethyl  (DEAE)-Sephadex, preparative polyacrylamide gel electrophoresis, and finally  chromatography on Sephadex G-100. These procedures resulted in a preparation of RBP  which was at least 98\% pure and which had been purified more than 1500-fold.  Purified RBP has alpha(1) mobility on electrophoresis and has a molecular weight of  approximately 21,000-22,000. There appears to be one binding site for retinol per  molecule of RBP. Solutions of RBP are fluorescent (characteristic of retinol) and  have ultraviolet absorption spectra with peaks at 330 mmu (resulting from the bound  retinol) and at 280 mmu. There are no fatty acid or fatty acyl chains present in  purified RBP. The usual concentration of RBP in plasma is of the order of 3-4 mg/100  ml. In plasma, RBP apparently circulates as a complex, together with another, larger  protein with prealbumin mobility on electrophoresis. The RBP-prealbumin complex  remains intact during Cohn fractionation and during chromatography on Sephadex and  on DEAE-Sephadex columns. The complex dissociates during gel electrophoresis,  permitting the isolation and subsequent purification of each of the components. The  complex is again formed by mixing together solutions of the separated RBP and of  prealbumin. Retinol transport in plasma thus appears to involve both a lipid-protein  (retinol-RBP) interaction and a protein-protein (RBP-prealbumin) interaction.},
	language = {eng},
	number = {9},
	journal = {The Journal of clinical investigation},
	author = {Kanai, M. and Raz, A. and Goodman, D. S.},
	month = sep,
	year = {1968},
	pmid = {5675424},
	pmcid = {PMC297364},
	keywords = {Chromatography, Humans, Binding Sites, Adult, Electrophoresis, Molecular Weight, *Biological Transport, Blood Proteins/*analysis, Carbon Isotopes, Spectrum Analysis, Vitamin A/*blood},
	pages = {2025--2044},
}

