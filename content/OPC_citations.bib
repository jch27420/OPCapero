\_eprinttype: pmid},
	keywords = {Humans, Receptors, Cultured, Digoxin, Neoplasm, Tumor Cells, RNA, Genes, Messenger, Adenocarcinoma, Down-Regulation, Caco-2 Cells, Colonic Neoplasms, MDR, Pregnane X Receptor, Steroid, Up-Regulation},
	pages = {116--120},
