@article{weng_origin_2010,
	title = {The {Origin} and {Evolution} of {Lignin} {Biosynthesis}},
	volume = {187},
	issn = {1469-8137},
	doi = {10.1111/j.1469-8137.2010.03327.x},
	abstract = {SUMMARY: Lignin, a phenolic polymer derived mainly from hydroxycinnamyl alcohols, is ubiquitously present in tracheophytes. The development of lignin biosynthesis has been considered to be one of the key factors that allowed land plants to flourish in terrestrial ecosystems. Lignin provides structural rigidity for tracheophytes to stand upright, and strengthens the cell wall of their water-conducting tracheary elements to withstand the negative pressure generated during transpiration. In this review, we discuss a number of aspects regarding the origin and evolution of lignin biosynthesis during land plant evolution, including the establishment of its monomer biosynthetic scaffold, potential precursors to the lignin polymer, as well as the emergence of the polymerization machinery and regulatory system. The accumulated knowledge on the topic, as summarized here, provides us with an evolutionary view on how this complex metabolic system emerged and developed.},
	number = {2},
	journal = {The New Phytologist},
	author = {Weng, Jing-Ke and Chapple, Clint},
	month = jul,
	year = {2010},
	note = {\_eprint: 20642725
\_eprinttype: pmid},
