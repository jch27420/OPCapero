@article{ghyselinck_retinoic_2019,
	title = {Retinoic acid signaling pathways.},
	volume = {146},
	copyright = {© 2019. Published by The Company of Biologists Ltd.},
	issn = {1477-9129 0950-1991 0950-1991},
	doi = {10.1242/dev.167502},
	abstract = {Retinoic acid (RA), a metabolite of retinol (vitamin A), functions as a ligand for  nuclear RA receptors (RARs) that regulate development of chordate animals. RA-RARs  can activate or repress transcription of key developmental genes. Genetic studies in  mouse and zebrafish embryos that are deficient in RA-generating enzymes or RARs have  been instrumental in identifying RA functions, revealing that RA signaling regulates  development of many organs and tissues, including the body axis, spinal cord,  forelimbs, heart, eye and reproductive tract. An understanding of the normal  functions of RA signaling during development will guide efforts for use of RA as a  therapeutic agent to improve human health. Here, we provide an overview of RA  signaling and highlight its key functions during development.},
	language = {eng},
	number = {13},
	journal = {Development (Cambridge, England)},
	author = {Ghyselinck, Norbert B. and Duester, Gregg},
	month = jul,
	year = {2019},
