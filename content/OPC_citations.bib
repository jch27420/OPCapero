	keywords = {Base Sequence, Molecular Sequence Data, RNA, Conserved Sequence, Substrate Specificity, Mutagenesis, Directed Molecular Evolution, Nucleic Acid Conformation, RNA-Dependent RNA Polymerase, RNA, Catalytic, Sequence Analysis, RNA, Templates, Genetic},
	pages = {1319--1325},
}

@article{zaug_intervening_1986,
